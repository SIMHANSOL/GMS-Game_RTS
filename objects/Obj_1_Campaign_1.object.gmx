        Obj_Game_System.view_move_y = 1326;
    }
    else if (text_switch == 18)
    {
        Obj_Game_System.view_switch = true; 
        Obj_Game_System.view_move_x = 0;
        Obj_Game_System.view_move_y = 0;
    }
    else if (text_switch == 19)
    {
